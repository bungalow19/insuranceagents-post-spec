			<enumeration value="$ 3 Million"/>
			<enumeration value="$ 4 Million"/>
			<enumeration value="$ 5 Million"/>
			<enumeration value="$ 6 Million"/>
			<enumeration value="$ 7 Million"/>
			<enumeration value="$ 8 Million"/>
			<enumeration value="$ 9 Million"/>
		</restriction>
	</simpleType>
	<simpleType name="gender_type">
		<restriction base="string">
			<enumeration value="M"/>
			<enumeration value="F"/>
		</restriction>
	</simpleType>
